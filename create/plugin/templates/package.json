{
  "name": "{{namespace}}/plugin-{{name}}",
  "version": "{{version}}",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "author": "{{{author}}}",
{{#if typescript}}
  "main": "index",
  "scripts":{
     "prepare":"tsc"
   },
{{else}}
"main": "src",

{{/if}}
  "keywords": [
    "mrbuilder",
    "{{name}}"
  ]
{{#if cli}}
  ,"bin": {
    "{{namespace}}-{{name}}": "./bin/cli.js"
  }
{{/if}}
{{#if babel}}
  ,"mrbuilder": {
    "options": {
      "@babel": "./src/babel"
    }
  }
{{/if}}
}