{
  "name": "@mrbuilder/example-lib-everything",
  "description": "An everything example",
  "version": "4.2.1",
  "private": true,
  "devDependencies": {
    "@mrbuilder/cli": "^4.2.1",
    "@mrbuilder/plugin-env": "^4.2.0",
    "@mrbuilder/plugin-graphql": "^4.2.1",
    "@mrbuilder/plugin-lodash": "^4.2.0",
    "@mrbuilder/plugin-markdown": "^4.2.1",
    "@mrbuilder/plugin-moment": "^4.2.0",
    "@mrbuilder/plugin-stylus": "^4.2.1",
    "@mrbuilder/plugin-typescript": "^4.2.1",
    "@mrbuilder/plugin-worker": "^4.2.0",
    "@mrbuilder/plugin-yaml": "^4.2.0",
    "@mrbuilder/plugin-prettier": "^4.2.0"

  },
  "scripts": {
    "analyze": "mrbuilder",
    "prepare": "mrbuilder-prettier && mrbuilder",
    "start": "mrbuilder",
    "test": "mrbuilder-karma-test",
    "karma": "mrbuilder",
    "lint": "mrbuilder",
    "storybook": "mrbuilder",
    "prettier": "mrbuilder",
    "storybook:start": "mrbuilder --@mrbuilder/plugin-storybook.parameters.theme=dark  "
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "source": "src",
  "main": "lib",
  "mrbuilder": {
    "presets": [
      "@mrbuilder/preset-lib"
    ],
    "plugins": [
      [
        "@mrbuilder/plugin-css",
        {
          "modules": true
        }
      ],
      [
        "@mrbuilder/plugin-env",
        {
          "environment": [
            "NODE_ENV",
            "USER"
          ]
        }
      ],
      "@mrbuilder/plugin-yaml",
      "@mrbuilder/plugin-markdown",
      "@mrbuilder/plugin-lodash",
      "@mrbuilder/plugin-env",
      "@mrbuilder/plugin-moment",
      "@mrbuilder/plugin-stylus",
      "@mrbuilder/plugin-worker",
      "@mrbuilder/plugin-typescript",
      "@mrbuilder/plugin-graphql",
      "@mrbuilder/plugin-prettier"
    ],
    "env": {
      "storybook": {
        "plugins": [
          [
            "@mrbuilder/plugin-storybook",
            {
              "stories": [
                "../example-lib/src/**/*.stories.jsx"
              ]
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          [
            "@mrbuilder/plugin-html",
            false
          ]
        ]
      }
    }
  },
  "license": "0BSD"
}
