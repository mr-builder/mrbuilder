.tiny, .help
    display none
    position absolute
    background transparent
    border 1px solid #ccc
    border-radius 3px
    font-size 8px
    width 100px
    transform translate3d(-60%, 17px, 0)

.textarea

    display none
    -webkit-appearance: none
    width 100%
    margin 0 0
    border none
    background white
    outline none
    text-align left
    min-width: 250px
    min-height: 250px
    border: 2px solid #ccc
    z-index: 3

.value-container:hover .textarea, .textarea:hover
    display inline-block
    position fixed

.textarea
    &.valid
        border 2px solid lightblue
    &.invalid
        border 2px solid darkred

.input
    -webkit-appearance: none
    width 100%
    margin 0 0
    border none
    background transparent
    outline none
    text-align center

.prop-name:hover + .help, .help:hover
    display inline-block
    position fixed
    background #fff
    border 1px solid #ccc
    padding 3px
    border-radius 3px
    font-family Roboto, sans-serif
    font-size 12px
    margin-top -30px
    margin-left -50px
    width auto
    transform none

.value-container:hover > .tiny, :hover + .tiny, .tiny:hover
    display inline-block

.sample
    box-sizing border-box
    color rgb(36, 41, 46)
    height 49px
    overflow-wrap break-word
    text-decoration none solid rgb(36, 41, 46)
    text-size-adjust 100%
    word-wrap break-word
    column-rule-color rgb(36, 41, 46)
    perspective-origin 446px 24.5px
    transform-origin 446px 24.5px
    caret-color rgb(36, 41, 46)
    border 0 none rgb(36, 41, 46)
    font normal normal normal normal 14px / 21px -apple-system, system-ui, "Segoe UI", Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol"
    outline rgb(36, 41, 46) none 0

.code
    position relative
    box-sizing border-box
    color rgb(36, 41, 46)
    height 49px
    text-decoration none solid rgb(36, 41, 46)
    text-size-adjust 100%
    column-rule-color rgb(36, 41, 46)
    perspective-origin 446px 24.5px
    transform-origin 446px 24.5px
    caret-color rgb(36, 41, 46)
    background rgb(246, 248, 250) none repeat scroll 0% 0% / auto padding-box border-box
    border 0 none rgb(36, 41, 46)
    border-radius 3px 3px 3px 3px
    font normal normal normal normal 11.9px / 17.255px SFMono-Regular, Consolas, "Liberation Mono", Menlo, Courier, monospace
    margin 0
    outline rgb(36, 41, 46) none 0
    overflow auto
    padding 16px

.component-container
    box-sizing border-box
    color rgb(34, 134, 58)
    text-decoration none solid rgb(34, 134, 58)
    text-size-adjust 100%
    white-space pre
    column-rule-color rgb(34, 134, 58)
    caret-color rgb(34, 134, 58)
    border 0 none rgb(34, 134, 58)
    font normal normal normal normal 11.9px / 17.255px SFMono-Regular, Consolas, "Liberation Mono", Menlo, Courier, monospace
    outline rgb(34, 134, 58) none 0

.component, .value-value
    box-sizing border-box
    color rgb(0, 92, 197)
    text-decoration none solid rgb(0, 92, 197)
    text-size-adjust 100%
    white-space pre
    column-rule-color rgb(0, 92, 197)
    caret-color rgb(0, 92, 197)
    border 0 none rgb(0, 92, 197)
    font normal normal normal normal 11.9px / 17.255px SFMono-Regular, Consolas, "Liberation Mono", Menlo, Courier, monospace
    outline rgb(0, 92, 197) none 0

.component
    cursor pointer

