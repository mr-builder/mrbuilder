{
  "name": "mrbuilder-plugin-karma",
  "version": "3.3.1-alpha.0",
  "description": "Tools for developing with mrbuilder and karma",
  "scripts": {
    "test": "mrbuilder-mocha"
  },
  "bin": {
    "mrbuilder-karma-cli": "./bin/cli.js"
  },
  "dependencies": {
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.0-rc.5",
    "mrbuilder-optionsmanager": "^3.3.1-alpha.0",
    "mrbuilder-plugin-enzyme": "^3.3.1-alpha.0",
    "mrbuilder-plugin-mocha": "^3.3.1-alpha.0",
    "mrbuilder-plugin-webpack": "^3.3.1-alpha.0"
  },
  "source": "src",
  "main": "src",
  "mrbuilder": {
    "alias": {
      "mainFields": "Sets main fields for easier debugging"
    },
    "plugins": [
      "mrbuilder-plugin-browserslist",
      "mrbuilder-plugin-webpack",
      "mrbuilder-plugin-enzyme",
      "mrbuilder-plugin-mocha"
    ]
  },
  "license": "0BSD",
  "peerDependencies": {
    "mrbuilder": "3.x"
  },
  "gitHead": "dcf4a516478af2e154c52e935f1be6ae1518938a"
}
