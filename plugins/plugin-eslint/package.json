{
  "name": "@mrbuilder/plugin-eslint",
  "description": "Provides eslint support",
  "version": "4.4.6",
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-react": "^7.19.0"
  },
  "source": "src",
  "main": "src",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT",
  "mrbuilder": {
    "options": {
      "ext": "js,jsx",
      "fix": true,
      "exclude": "/node_modules/",
      "cache": true,
      "enforce": "pre",
      "configFile": "@mrbuilder/plugin-eslint/src/eslint.js"
    }
  }
}
