{
  "name": "@mrbuilder/plugin-filetypes",
  "version": "4.1.0",
  "description": "Loading file types via webpack",
  "dependencies": {
    "file-loader": "^6.0.0",
    "url-loader": "^4.0.0"
  },
  "source": "src",
  "main": "src",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "test": "/\\.(png|je?pg|gif?f|bmp|ppm|bpg|mpe?g)$/",
      "loader": "file-loader"
    }
  },
  "license": "MIT"
}