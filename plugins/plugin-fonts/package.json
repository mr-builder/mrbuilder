{
  "name": "@mrbuilder/plugin-fonts",
  "version": "4.4.7-alpha.6",
  "description": "Loadin fonts types via webpack",
  "scripts": {
    "test": "mrbuilder-mocha"
  },
  "dependencies": {
    "@mrbuilder/plugin-filetypes": "^4.4.7-alpha.6"
  },
  "devDependencies": {
    "@mrbuilder/plugin-mocha": "^4.4.7-alpha.6"
  },
  "source": "src",
  "main": "src",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "loader": "url-loader",
      "limit": 10000,
      "fontTypes": [
        {
          "test": "/\\.(woff|woff2)(\\?v=\\d+\\.\\d+\\.\\d+)?$/",
          "mimetype": "application/font-woff"
        },
        {
          "test": "/\\.ttf(\\?v=\\d+\\.\\d+\\.\\d+)?$/",
          "mimetype": "application/octet-stream"
        },
        {
          "test": "/\\.eot(\\?v=\\d+\\.\\d+\\.\\d+)?$/",
          "loader": "file-loader"
        },
        {
          "test": "/\\.svg(\\?v=\\d+\\.\\d+\\.\\d+)?$/",
          "mimetype": "image/svg+xml"
        }
      ]
    }
  },
  "license": "MIT"
}
