{
  "name": "@mrbuilder/plugin-graphql",
  "version": "4.2.0-alpha.2",
  "description": "Loading graphql webpack",
  "dependencies": {
    "graphql-tag": "^2.9.2"
  },
  "devDependencies": {
    "@mrbuilder/plugin-mocha": "^4.2.0-alpha.2"
  },
  "source": "src",
  "main": "src",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "test": "/\\.(?:graphql|gql)s?$/",
      "loader": "graphql-tag/loader",
      "exclude": "/node_modules/"
    }
  },
  "license": "MIT"
}