{
  "name": "@mrbuilder/plugin-graphql",
  "version": "4.4.6",
  "description": "Loading graphql webpack",
  "dependencies": {
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-import-graphql": "^2.7.0",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.9.2"
  },
  "devDependencies": {
    "@mrbuilder/plugin-mocha": "^4.4.6"
  },
  "source": "src",
  "main": "src",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "@babel": [
        "babel-plugin-graphql-tag"
      ],
      "test": "/\\.(?:graphql|gql)s?$/",
      "loader": "graphql-tag/loader",
      "exclude": "/node_modules/"
    }
  },
  "license": "MIT"
}
