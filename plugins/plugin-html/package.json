{
  "name": "@mrbuilder/plugin-html",
  "version": "4.3.0-alpha.0",
  "description": "html plugin for mrbuilder",
  "dependencies": {
    "@mrbuilder/plugin-babel": "^4.3.0-alpha.0",
    "@mrbuilder/utils": "^4.3.0-alpha.0",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.0.4",
    "html-webpack-template": "^6.2.0"
  },
  "source": "src",
  "main": "src",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "publicPath": "public",
      "filename": "[name].html",
      "elementId": "content",
      "exported": true,
      "inlineManifest": true
    }
  },
  "license": "MIT"
}
