{
  "name": "@mrbuilder/plugin-html",
  "version": "4.2.0-alpha.1",
  "description": "html plugin for mrbuilder",
  "dependencies": {
    "@mrbuilder/plugin-babel": "^4.2.0-alpha.0",
    "@mrbuilder/utils": "^4.2.0-alpha.1",
    "glob": "^7.1.6",
    "html-webpack-plugin": "^4.0.3",
    "html-webpack-template": "^6.2.0"
  },
  "source": "src",
  "main": "src",
  "license": "0BSD",
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "gitHead": "8932fa60b6c6f6ed18b45918e7873c8e70b8d0be",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "publicPath": "public",
      "filename": "[name].html",
      "elementId": "content",
