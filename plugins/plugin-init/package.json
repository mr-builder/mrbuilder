{
  "name": "@mrbuilder/plugin-init",
  "version": "4.2.0-alpha.2",
  "main": "bin/mrbuilder-init",
  "dependencies": {
    "@mrbuilder/cli": "^4.2.0-alpha.2",
    "@mrbuilder/optionsmanager": "^4.2.0-alpha.2"
  },
  "scripts": {
    "mrbuilder-init": "./bin/mrbuilder-init.js"
  },
  "bin": {
    "mrbuilder-init": "./bin/init"
  },
  "mrbuilder": {
    "alias": {
      "name": "Name of the module [required]",
      "version": "Version of current module [default:0.0.1]",
      "type": "Is it an app, demo or lib [default:lib]",
      "description": "Description of project",
      "start": "Script to start defaults start [default:mrbuilder-webpack-dev-server]",
      "prepublish": "Script to run on publish [default:mrbuilder-webpack]",
      "clean": "Script to run on clean [default:mrbuilder-clean]",
      "test": "Script to run on test [default:mrbuilder-karma]",
      "karma": "Script to run for karma testing [default:mrbuilder-karma]",
      "plugins": "Plugins to enable by default",
      "presets": "Presets to enable by default",
      "rootRepository": "The project will be appended to this to create repository url",
      "rootDemo": "The project will be appended to this to create demo url"
    }
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT"
}