{
  "name": "@mrbuilder/plugin-less",
  "version": "4.1.1",
  "description": "less support for mrbuilder",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.1.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.1.0"
  },
  "scripts": {
    "test": "mrbuilder-mocha"
  },
  "source": "src",
  "main": "src",
  "mrbuilder": {
    "options": {
      "test": "/[.]less$/",
      "modules": true,
      "options": {
        "strictMath": true,
        "noIeCompat": true
      }
    },
    "plugins": [
      "@mrbuilder/plugin-css"
    ]
  },
  "license": "0BSD",
  "gitHead": "8932fa60b6c6f6ed18b45918e7873c8e70b8d0be",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  }
}
