{
  "name": "@mrbuilder/plugin-preact",
  "version": "4.2.0-alpha.2",
  "source": "src",
  "main": "src",
  "dependencies": {
    "preact": "^10.4.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "useCompat": true,
      "@babel": "@mrbuilder/plugin-preact/babel",
      "alias": {
        "react": "~preact/compat",
        "react-dom/test-utils": "~preact/test-utils",
        "react-dom": "~preact/compat",
        "react/": "~preact/compat/",
        "react-dom/": "~preact/compat/"
      }
    },
    "plugins": [
      "@mrbuilder/plugin-react"
    ]
  },
  "license": "MIT"
}