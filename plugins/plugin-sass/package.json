{
  "name": "@mrbuilder/plugin-sass",
  "version": "4.4.0",
  "description": "Sass plugin for mrbuilder",
  "main": "./src/index",
  "author": "Justin Spears <speajus@gmail.com>",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.4.0",
    "@mrbuilder/utils": "^4.4.0",
    "loader-utils": "^2.0.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2"
  },
  "keywords": [
    "subschema",
    "sass",
    "mrbuilder"
  ],
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "test": "/[.]s[ac]ss$/",
      "modules": true,
      "options": {
        "sourceMap": true
      }
    }
  },
  "license": "MIT"
}
