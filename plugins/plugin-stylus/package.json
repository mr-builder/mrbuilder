{
  "name": "@mrbuilder/plugin-stylus",
  "version": "4.4.7-alpha.7",
  "description": "Stylus plugin for mrbuilder",
  "main": "./src/index",
  "author": "Justin Spears <speajus@gmail.com>",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.4.7-alpha.7",
    "loader-utils": "^2.0.0",
    "nib": "^1.1.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2"
  },
  "keywords": [
    "subschema",
    "stylus",
    "mrbuilder"
  ],
  "mrbuilder": {
    "options": {
      "test": "/[.]styl$/",
      "modules": "/[.]stylm$/",
      "nib": true,
      "includeCss": true,
      "hoistAtRules": true,
      "compress": false,
      "sourceMap": true
    },
    "plugins": [
      "@mrbuilder/plugin-css"
    ]
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT"
}
