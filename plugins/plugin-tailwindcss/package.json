{
  "name": "@mrbuilder/plugin-tailwindcss",
  "version": "4.0.0-alpha.12",
  "description": "CSS support for mbuilder",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.0.0-alpha.12",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.1.4"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "main": "src",
  "source": "src",
  "license": "0BSD",
  "publishConfig": {
    "access": "public"
  },
  "mrbuilder": {
    "options": {
      "test": "/\\.twcss$/",
      "publicPath": "/public",
      "config": {
        "theme": {},
        "variants": {},
        "plugins": []
      }
    },
    "plugins": [
      [
        "@mrbuilder/plugin-css",
        true
      ]
    ]
  }
}
