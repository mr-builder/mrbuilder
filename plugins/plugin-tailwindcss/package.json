{
  "name": "@mrbuilder/plugin-tailwindcss",
  "version": "4.3.0-alpha.0",
  "description": "CSS support for mbuilder",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.3.0-alpha.0",
    "postcss-loader": "^3.0.0",
    "tailwindcss": "^1.2.0"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "main": "src",
  "source": "src",
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "options": {
      "test": "/\\.twcss$/",
      "publicPath": "/public",
      "config": {
        "theme": {},
        "variants": {},
        "plugins": []
      }
    },
    "plugins": [
      [
        "@mrbuilder/plugin-css",
        true
      ]
    ]
  },
  "license": "MIT"
}
