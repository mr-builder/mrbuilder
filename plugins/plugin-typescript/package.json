{
  "name": "@mrbuilder/plugin-typescript",
  "version": "4.2.0-alpha.2",
  "description": "Typescript plugin for mrbuilder",
  "main": "./src",
  "author": "Justin Spears <speajus@gmail.com>",
  "bin": {
    "mrbuilder-typescript": "./bin/cli.js"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@mrbuilder/cli": "^4.2.0-alpha.2",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.27",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@mrbuilder/cli": "^4.0.0-alpha.1"
  },
  "keywords": [
    "typescript",
    "mrbuilder"
  ],
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "plugin": "./src/index",
    "options": {
      "@babel": "./babel.js",
      "managed": false,
      "extensions": [
        ".ts",
        ".tsx"
      ]
    }
  },
  "license": "MIT"
}