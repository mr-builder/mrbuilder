{
  "name": "@mrbuilder/plugin-typescript",
  "version": "4.4.7-alpha.5",
  "description": "Typescript plugin for mrbuilder",
  "main": "./src",
  "author": "Justin Spears <speajus@gmail.com>",
  "bin": {
    "mrbuilder-typescript": "./bin/cli.js"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@mrbuilder/cli": "^4.4.7-alpha.5",
    "@types/node": "^13.9.8",
    "@types/react": "^16.9.27",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^8.0.14",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "keywords": [
    "typescript",
    "mrbuilder"
  ],
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "mrbuilder": {
    "plugin": "./src/index",
    "options": {
      "@babel": "./babel.js",
      "managed": false,
      "test": "/[.]tsx?$/",
      "extensions": [
        ".ts",
        ".tsx"
      ]
    }
  },
  "license": "MIT"
}
