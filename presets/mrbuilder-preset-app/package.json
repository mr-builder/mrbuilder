{
  "name": "mrbuilder-preset-app",
  "version": "3.1.2",
  "dependencies": {
    "mrbuilder-plugin-babel": "^3.1.2",
    "mrbuilder-plugin-browserslist": "^3.1.2",
    "mrbuilder-plugin-chunk": "^3.1.2",
    "mrbuilder-plugin-css": "^3.1.2",
    "mrbuilder-plugin-env": "^3.1.2",
    "mrbuilder-plugin-filetypes": "^3.1.2",
    "mrbuilder-plugin-fonts": "^3.1.2",
    "mrbuilder-plugin-hot": "^3.1.2",
    "mrbuilder-plugin-html": "^3.1.2",
    "mrbuilder-plugin-less": "^3.1.2",
    "mrbuilder-plugin-uglify": "^3.1.2",
    "mrbuilder-plugin-react": "^3.1.2",
    "mrbuilder-plugin-webpack": "^3.1.2",
    "mrbuilder-plugin-webpack-dev-server": "^3.1.2"
  },
  "peerDependencies": {
    "mrbuilder": "3.x"
  },
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-babel",
      "mrbuilder-plugin-browserslist",
      "mrbuilder-plugin-filetypes",
      "mrbuilder-plugin-fonts",
      "mrbuilder-plugin-env",
      "mrbuilder-plugin-less",
      "mrbuilder-plugin-react",
      [
        "mrbuilder-plugin-html",
        {
          "exported": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "mrbuilder-plugin-html",
            false
          ]
        ],
        "presets": [
          "mrbuilder-preset-test"
        ]
      },
      "development": {
        "presets": [
          "mrbuilder-preset-dev"
        ]
      },
      "production": {
        "plugins": [
          [
            "mrbuilder-plugin-hot",
            false
          ],
          [
            "mrbuilder-plugin-css",
            {
              "useNameHash": "css/[name].[hash].css"
            }
          ],
          [
            "mrbuilder-plugin-webpack",
            {
              "app": "app",
              "filename": "js/[name].[hash].js",
              "externalizePeers": false,
              "useExternals": false
            }
          ],
          [
            "mrbuilder-plugin-chunk",
            {
              "filename": "js/[name].[hash].js"
            }
          ],
          "mrbuilder-plugin-uglify"
        ]
      }
    }
  },
  "license": "0BSD",
  "gitHead": "8480796a0ee92932e218723e26f8221a855f4d4c"
}
