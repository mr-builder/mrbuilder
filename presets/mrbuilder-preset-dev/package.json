{
  "name": "mrbuilder-preset-dev",
  "version": "3.1.2",
  "dependencies": {
    "mrbuilder-plugin-webpack-dev-server": "^3.1.2",
    "mrbuilder-preset-app": "^3.1.2",
    "mrbuilder-plugin-eslint": "^3.1.2",
    "mrbuilder-plugin-react": "^3.1.2",
    "mrbuilder-plugin-hot": "^3.1.2",
    "mrbuilder-plugin-css": "^3.1.2"
  },
  "peerDependencies": {
    "mrbuilder": "3.x"
  },
  "mrbuilder": {
    "presets": [
      "mrbuilder-preset-app"
    ],
    "plugins": [
      "mrbuilder-plugin-react",
      "mrbuilder-plugin-eslint",
      [
        "mrbuilder-plugin-html",
        {
          "exported": true
        }
      ],
      "mrbuilder-plugin-hot",
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": "[name].css",
          "useStyleLoader": true
        }
      ],
      [
        "mrbuilder-plugin-webpack",
        {
          "useExternals": false,
          "externalizePeers": false
        }
      ],
      "mrbuilder-plugin-webpack-dev-server"
    ]
  },
  "license": "0BSD",
  "gitHead": "8480796a0ee92932e218723e26f8221a855f4d4c"
}
