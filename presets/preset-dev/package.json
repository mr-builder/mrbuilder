{
  "name": "@mrbuilder/preset-dev",
  "version": "4.0.0-alpha.4",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.0.0-alpha.4",
    "@mrbuilder/plugin-eslint": "^4.0.0-alpha.4",
    "@mrbuilder/plugin-hot": "^4.0.0-alpha.2",
    "@mrbuilder/plugin-react": "^4.0.0-alpha.4",
    "@mrbuilder/plugin-webpack-dev-server": "^4.0.0-alpha.4",
    "@mrbuilder/preset-app": "^4.0.0-alpha.4"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "mrbuilder": {
    "presets": [
      "@mrbuilder/preset-app"
    ],
    "plugins": [
      "@mrbuilder/plugin-react",
      "@mrbuilder/plugin-eslint",
      [
        "@mrbuilder/plugin-html",
        {
          "exported": true
        }
      ],
      "@mrbuilder/plugin-hot",
      [
        "@mrbuilder/plugin-css",
        {
          "useNameHash": "[name].css",
          "useStyleLoader": true
        }
      ],
      [
        "@mrbuilder/plugin-webpack",
        {
          "useExternals": false,
          "externalizePeers": false
        }
      ],
      "@mrbuilder/plugin-webpack-dev-server"
    ]
  },
  "license": "0BSD",
  "gitHead": "771c5d4ad70156a7c7e8730c68369ed872e2cfec",
  "publishConfig": {
    "access": "public"
  }
}
