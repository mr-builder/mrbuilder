{
  "name": "@mrbuilder/preset-dev",
  "version": "4.2.0-alpha.2",
  "dependencies": {
    "@mrbuilder/plugin-css": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-eslint": "^4.1.0",
    "@mrbuilder/plugin-hot": "^4.1.0",
    "@mrbuilder/plugin-react": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-webpack-dev-server": "^4.2.0-alpha.2",
    "@mrbuilder/preset-app": "^4.2.0-alpha.2"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "mrbuilder": {
    "presets": [
      "@mrbuilder/preset-app"
    ],
    "plugins": [
      "@mrbuilder/plugin-react",
      "@mrbuilder/plugin-eslint",
      [
        "@mrbuilder/plugin-html",
        {
          "exported": true
        }
      ],
      "@mrbuilder/plugin-hot",
      [
        "@mrbuilder/plugin-css",
        {
          "useNameHash": "[name].css",
          "useStyleLoader": true
        }
      ],
      [
        "@mrbuilder/plugin-webpack",
        {
          "useExternals": false,
          "externalizePeers": false
        }
      ],
      "@mrbuilder/plugin-webpack-dev-server"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT"
}