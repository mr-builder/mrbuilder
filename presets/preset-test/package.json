{
  "name": "@mrbuilder/preset-test",
  "version": "4.2.0-alpha.2",
  "dependencies": {
    "@mrbuilder/plugin-babel": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-css": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-enzyme": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-karma": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-mocha": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-react": "^4.2.0-alpha.2",
    "@mrbuilder/plugin-webpack": "^4.2.0-alpha.2"
  },
  "peerDependencies": {
    "@mrbuilder/cli": "^4.0.0"
  },
  "mrbuilder": {
    "plugins": [
      "@mrbuilder/plugin-babel",
      "@mrbuilder/plugin-react",
      [
        "@mrbuilder/plugin-webpack",
        {
          "useExternals": false,
          "externalizePeers": false
        }
      ],
      [
        "@mrbuilder/plugin-html",
        false
      ],
      [
        "@mrbuilder/plugin-css",
        {
          "useNameHash": "[id].[name].css",
          "useStyleLoader": true
        }
      ],
      [
        "@mrbuilder/plugin-html",
        false
      ],
      [
        "@mrbuilder/plugin-hot",
        false
      ],
      "@mrbuilder/plugin-enzyme",
      "@mrbuilder/plugin-mocha",
      "@mrbuilder/plugin-webpack",
      "@mrbuilder/plugin-karma"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "@mrbuilder/plugin-karma",
            {
              "browsers": [
                "SimpleHeadlessChrome"
              ]
            }
          ]
        ]
      }
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "license": "MIT"
}